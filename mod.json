{
	"geode": "4.0.0-alpha.1",
	"gd": {
		"win": "2.206",
		"android": "2.206",
		"mac": "2.206"
	},
	"version": "v1.2.1",
	"id": "khronophobia.shortcuts_menu",
	"name": "Shortcuts Menu",
	"developer": "Khronophobia",
	"description": "Adds a shortcut button that takes you to various parts of the game.",
	"links": {
		"source": "https://github.com/Khronophobia/Shortcuts-Menu"
	},
	"resources": {
		"spritesheets": {
			"ShortcutsMenu": [
				"resources/ShortcutsMenu/*.png"
			]
		}
	},
	"dependencies": [
		{
			"id": "geode.node-ids",
			"version": ">=1.12.0",
			"importance": "required"
		}
	],
	"settings": {
		"show-spoilers": {
			"name": "Show Spoilers",
			"type": "string",
			"description": "Whether to show shortcuts you haven't unlocked yet. Setting this to <cy>Show</c> does <cr>NOT</c> make them accessible.",
			"default": "Hide",
			"one-of": [
				"Hide", "As Silhouette", "Show"
			]
		},
		"enable-menulayer": {
			"name": "Enable in MenuLayer",
			"type": "bool",
			"default": true
		},
		"enable-creatorlayer": {
			"name": "Enable in CreatorLayer",
			"type": "bool",
			"default": true
		},
		"enable-editlevellayer": {
			"name": "Enable in EditLevelLayer",
			"type": "bool",
			"default": true
		},
		"enable-levelbrowserlayer": {
			"name": "Enable in LevelBrowserLayer",
			"type": "bool",
			"default": true
		},
		"enable-levelsearchlayer": {
			"name": "Enable in LevelSearchLayer",
			"type": "bool",
			"default": true
		},
		"enable-levelinfolayer": {
			"name": "Enable in LevelInfoLayer",
			"type": "bool",
			"default": true
		}
	},
	"tags": [
		"enhancement"
	]
}